{
  "manifest_version": 3,
  "name": "Tech News Feed",
  "version": "1.0",
  "description": "View latest tech news in a clean dark theme interface",
  "permissions": ["storage", "tabs"],
  "background": {
    "service_worker": "src/background.js"
  },
  "chrome_url_overrides": {
    "newtab": "src/index.html"
  },
  "action": {
    "default_icon": {
      "16": "src/icon16.png",
      "48": "src/icon48.png",
      "128": "src/icon128.png"
    }
  },
  "icons": {
    "16": "src/icon16.png",
    "48": "src/icon48.png",
    "128": "src/icon128.png"
  }
}
