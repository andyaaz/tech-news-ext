{
  "manifest_version": 3,
  "name": "Tech News Feed",
  "version": "1.0",
  "description": "View latest tech news in a clean dark theme interface",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": {
      "16": "src/icon16.png",
      "48": "src/icon48.png",
      "128": "src/icon128.png"
    }
  },
  "icons": {
    "16": "src/icon16.png",
    "48": "src/icon48.png",
    "128": "src/icon128.png"
  }
}
